[user]
	name = Luis Rampolla
	email = 33848069+rampollaluis@users.noreply.github.com
[init]
	defaultBranch = development
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
[core]
	pager = diff-so-fancy | less --tabs=4 -RFX
[interactive]
	diffFilter = diff-so-fancy --patch
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	func = 146 bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
